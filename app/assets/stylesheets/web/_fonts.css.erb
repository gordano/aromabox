@font-face {
  font-family: 'Museo Sans Cyrl';
  src:  url('<%= font_path "museo/thin/MuseoSansCyrl_300.eot" %>');
  src:  url('<%= font_path "museo/thin/MuseoSansCyrl_300.eot" %>?#iefix') format('embedded-opentype'),
        url('<%= font_path "museo/thin/MuseoSansCyrl_300.woff2" %>') format('woff2'),
        url('<%= font_path "museo/thin/MuseoSansCyrl_300.woff" %>') format('woff'),
        url('<%= font_path "museo/thin/MuseoSansCyrl_300.ttf" %>') format('truetype');
  font-weight: 300;
  font-style: normal;
}

